<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AdminController">
   <children>
      <Text fx:id="userListText" layoutX="71.0" layoutY="92.0" strokeType="OUTSIDE" strokeWidth="0.0" text="List of users">
         <font>
            <Font size="72.0" />
         </font>
      </Text>
      <ListView fx:id="userListView" layoutX="44.0" layoutY="126.0" prefHeight="620.0" prefWidth="430.0" />
      <TextField fx:id="addUser" layoutX="527.0" layoutY="381.0" prefHeight="38.0" prefWidth="420.0" onAction="#addUser" />
      <Text layoutX="595.0" layoutY="367.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Create a new user">
         <font>
            <Font size="36.0" />
         </font>
      </Text>
      <Button fx:id="createButton" layoutX="704.0" layoutY="431.0" mnemonicParsing="false" onAction="#handleEvent" prefHeight="25.0" prefWidth="65.0" text="Create" />
      <Button fx:id="deleteButton" layoutX="233.0" layoutY="753.0" mnemonicParsing="false" onAction="#handleEvent" text="Delete" />
      <Button fx:id="logoutButton" layoutX="920.0" layoutY="753.0" mnemonicParsing="false" onAction="#handleEvent" text="Logout" />
   </children>
</AnchorPane>
